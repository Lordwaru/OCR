{{range $i,$element := .accounts}}    

<div class="row">
    <div class="col s12 m3">
      <div class="card">
        <div class="card-content  teal darken-1 white-text">
          <span class="card-title">{{$element.Origin_data}}</span>         
        </div>
        <div class="card-action">
          <table class="striped">
           <tr class="teal lighten-5">              
              <th>Account</th>
              <th>Status</th>
           </tr>
           <tbody>
            {{range $j,$accountStatus := $element.Accounts}}
              <tr>
                <td>{{$accountStatus.AccountNumber}}</td>
                <td>{{$accountStatus.Status}}</td>
              </tr>
            {{end}}
          </tbody>          
          </table>
        </div>
      </div>
    </div>
  </div>

    

        
                
{{end}}

<style>
table, th, td {
  border: 1px solid black;
}

h2{
    text-overflow:ellipsis;
}


